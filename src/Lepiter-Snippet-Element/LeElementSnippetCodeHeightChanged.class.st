Class {
	#name : #LeElementSnippetCodeHeightChanged,
	#superclass : #LeElementSnippetAnnouncement,
	#instVars : [
		'codeHeight'
	],
	#category : #'Lepiter-Snippet-Element-Events'
}

{ #category : #accessing }
LeElementSnippetCodeHeightChanged >> codeHeight [
	^ codeHeight
]

{ #category : #accessing }
LeElementSnippetCodeHeightChanged >> codeHeight: aHeight [
	codeHeight := aHeight
]
